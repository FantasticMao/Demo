root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = false
indent_style = space
indent_size = 4

[*.go]
indent_style = tab
tab_width = 4

[*.java]
indent_style = space
indent_size = 4

[{*.js, *.json}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true
ij_continuation_indent_size = 4

[{*.cql, *.ddl, *.sql}]
indent_style = space
indent_size = 2